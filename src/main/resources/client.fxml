<AnchorPane prefHeight="1000.0" prefWidth="1200.0" xmlns:ID="http://javafx.com/javafx/9.0.4" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
       <Pane fx:id="pane" layoutY="1.0" prefHeight="1000.0" prefWidth="1200.0">
