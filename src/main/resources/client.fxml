           <Label fx:id="lblparam3" layoutX="20.0" layoutY="162.0" text="Label"/>
           <Label fx:id="lblparam4" layoutX="353.0" layoutY="76.0" text="Label"/>
           <Label fx:id="lblparam5" layoutX="353.0" layoutY="120.0" text="Label"/>
           <Label fx:id="lblparam6" layoutX="353.0" layoutY="162.0" text="Label"/>


           <Button fx:id="btnSearch" layoutX="280.0" layoutY="199.0" mnemonicParsing="false" onAction="#clickSearch"
                   text="Search"/>
           <Label fx:id="desc" layoutX="24.0" layoutY="41.0" text="Description : "/>
           <TextArea fx:id="AreaResult" layoutX="16.0" layoutY="232.0" prefHeight="152.0" prefWidth="590.0"/>


       </Pane>

   </children>
