                       </items>
                   </Menu>
                   <Menu mnemonicParsing="false" text="Setting">
                       <items>
                           <MenuItem mnemonicParsing="false" onAction="#click_admin" text="Admin"/>
                       </items>
                   </Menu>
               </menus>
           </MenuBar>

           <TextField fx:id="txtParam1" layoutX="113.0" layoutY="71.0"/>
           <TextField fx:id="txtParam2" layoutX="113.0" layoutY="115.0"/>
           <TextField fx:id="txtParam3" layoutX="113.0" layoutY="157.0"/>
           <TextField fx:id="txtParam4" layoutX="419.0" layoutY="71.0"/>
